{
	"$schema": "https://biomejs.dev/schemas/2.2.2/schema.json",
	"assist": {
		"actions": {
			"source": {
				"organizeImports": "on",
				"recommended": true,
				"useSortedAttributes": "on",
				"useSortedKeys": "on",
				"useSortedProperties": "on"
			}
		},
		"enabled": true
	},
	"files": {
		"ignoreUnknown": false
	},
	"formatter": {
		"enabled": true,
		"indentStyle": "tab"
	},
	"javascript": {
		"formatter": {
			"quoteStyle": "double",
			"semicolons": "asNeeded"
		},
		"parser": {
			"unsafeParameterDecoratorsEnabled": true
		}
	},
	"linter": {
		"enabled": true,
		"rules": {
			"complexity": {
				"noUselessConstructor": {
					"fix": "safe",
					"level": "error",
					"options": {}
				},
				"useLiteralKeys": "off"
			},
			"correctness": {
				"noUnusedFunctionParameters": "error",
				"noUnusedImports": {
					"fix": "safe",
					"level": "error",
					"options": {}
				},
				"noUnusedLabels": "error",
				"noUnusedPrivateClassMembers": "error",
				"noUnusedVariables": "error"
			},
			"performance": {
				"noAccumulatingSpread": "off"
			},
			"recommended": true,
			"style": {
				"useImportType": "off"
			},
			"suspicious": {
				"noExplicitAny": "info"
			}
		}
	},
	"vcs": {
		"clientKind": "git",
		"enabled": true,
		"useIgnoreFile": true
	}
}